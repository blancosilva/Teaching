%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%% Page 5
\noindent{\large\bf MATH 142}\hfill{\large\bf Final Exam.}\hfill{\large\bf
  Spring 2012}\hfill{\large\bf Page 5/6}\hrule
  
\bigskip

{\problem[10 pts] \em Use the \textbf{root test} to determine whether
  the series $\displaystyle{\sum_{n=1}^\infty \Big( \frac{3n+2}{2n-1}
    \Big)^n}$ converges.  If the test is inconclusive, then say so.} 
\vspace{9cm}
\hrule{\problem[10 pts] \em Classify the series
  $\displaystyle{\sum_{n=1}^\infty (-1)^n\, \frac{4n^2+1}{n^3+2}}$ as
  absolutely convergent, convergent or divergent.} 

\newpage

%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%% Page 6
\noindent{\large\bf MATH 142}\hfill{\large\bf Final Exam.}\hfill{\large\bf
  Spring 2012}\hfill{\large\bf Page 6/6}\hrule
{\problem[15 pts] \em Find the radius of convergence and interval of
  convergence of the series}
\begin{equation*}
\sum_{n=0}^\infty \frac{(-3)^n x^n}{\sqrt{n+1}}
\end{equation*}

